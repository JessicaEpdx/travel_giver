<% render "layouts/errors", :object => @country %>

<%= bootstrap_form_for @country do |f| %>
  <%= f.text_field :c_name, label: "Country name" %>
  <br>
  <%= f.text_field :continent, label: "Continent" %>
  <br>
  <%= f.text_area :c_description, label: "Describe this country" %>
  <br>
  <%= f.text_field :project_image_url, label: "Add a screen shot of the project" %>
  <br>
  <%= f.text_field :project_link, label: "Add a link to the project" %>
  <%= f.submit "Add project", class: "btn btn-success" %>
<% end %>
<br>
<%= link_to "Return to home page", skills_path %>
